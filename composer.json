{
    "name": "jbickar/bobsbullets",
    "description": "Website for bobsbullets.net.",
    "type": "project",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "drush/drush": "~9.0.0"
    },
    "require": {
        "composer/installers": "^1.0",
        "drupal-composer/drupal-scaffold": "^2.0.0",
        "cweagans/composer-patches": "^1.6.0",
        "oomphinc/composer-installers-extender": "^1.1",
        "drupal/core": "~8.9.0",
        "drupal/migrate_upgrade": "3.0-rc2",
        "drupal/migrate_plus": "4.0-beta1",
        "drupal/migrate_tools": "4.0-beta1",
        "drupal/migrate_manifest": "1.5",
        "drupal/asset_injector": "2.1",
        "drupal/google_analytics": "2.2",
        "drupal/at_tools": "2.0",
        "drupal/adaptivetheme": "2.0",
        "drupal/pixture_reloaded": "1.0-rc1",
        "drupal/admin_toolbar": "^2.3"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "scripts": {
      "init-lando": [
        "cp lando/example.env .env",
        "cp lando/example.lando.yml .lando.yml",
        "cp lando/example.php.ini lando/php.ini"
      ]
    },
    "config": {
        "bin-dir": "bin/"
    },
    "extra": {
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "enable-patching": true
    }
}
